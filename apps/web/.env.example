# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/ai_eval_platform"

# Redis
REDIS_URL="redis://localhost:6379"

# NextAuth
NEXTAUTH_SECRET="your-32-character-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# Sandbox (dify-sandbox)
SANDBOX_URL="http://localhost:8194"
SANDBOX_API_KEY="dify-sandbox"

# FastGPT MongoDB 连接
# 用于读取 FastGPT 模型配置和写入团队管理数据（成员分组、组织架构、协作者权限）
FASTGPT_MONGODB_URI="mongodb://user:password@host:port/fastgpt?authSource=admin&directConnection=true"
ENABLE_FASTGPT_MODELS="true"

# FastGPT 模型同步间隔（毫秒），默认 5 分钟
FASTGPT_SYNC_INTERVAL="300000"

# OneHub AI 代理（用于调用 FastGPT 模型）
OPENAI_BASE_URL="http://onehub:3000/v1"
CHAT_API_KEY="sk-your-api-key"

# 或者使用独立的 MongoDB URI（如果不使用 FastGPT）
# MONGODB_URI="mongodb://localhost:27017/prompt_tool?authSource=admin"

# Optional
NODE_ENV="development"
LOG_LEVEL="debug"
